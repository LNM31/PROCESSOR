#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5c3d06f23100 .scope module, "memory_load_tb" "memory_load_tb" 2 3;
 .timescale -9 -12;
v0x5c3d06f50df0_0 .var "addr", 8 0;
v0x5c3d06f50ed0_0 .var "clk", 0 0;
v0x5c3d06f50fa0_0 .var "din", 15 0;
v0x5c3d06f510a0_0 .net "dout", 15 0, v0x5c3d06f50800_0;  1 drivers
v0x5c3d06f51170_0 .var/i "i", 31 0;
v0x5c3d06f51210_0 .var "read", 0 0;
v0x5c3d06f512b0_0 .var "rst_b", 0 0;
v0x5c3d06f51380_0 .var "write", 0 0;
S_0x5c3d06ef1490 .scope module, "mem" "memory_512x16" 2 15, 3 1 0, S_0x5c3d06f23100;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_b";
    .port_info 2 /INPUT 1 "write";
    .port_info 3 /INPUT 1 "read";
    .port_info 4 /INPUT 9 "addr";
    .port_info 5 /INPUT 16 "din";
    .port_info 6 /OUTPUT 16 "dout";
P_0x5c3d06ef1620 .param/l "AW" 0 3 2, +C4<00000000000000000000000000001001>;
P_0x5c3d06ef1660 .param/l "DW" 0 3 3, +C4<00000000000000000000000000010000>;
P_0x5c3d06ef16a0 .param/str "INIT_FILE" 0 3 4, "program.hex";
v0x5c3d06f23290_0 .net "addr", 8 0, v0x5c3d06f50df0_0;  1 drivers
v0x5c3d06f50650_0 .net "clk", 0 0, v0x5c3d06f50ed0_0;  1 drivers
v0x5c3d06f50710_0 .net "din", 15 0, v0x5c3d06f50fa0_0;  1 drivers
v0x5c3d06f50800_0 .var "dout", 15 0;
v0x5c3d06f508e0_0 .var/i "i", 31 0;
v0x5c3d06f50a10 .array "mem", 511 0, 15 0;
v0x5c3d06f50ad0_0 .net "read", 0 0, v0x5c3d06f51210_0;  1 drivers
v0x5c3d06f50b90_0 .net "rst_b", 0 0, v0x5c3d06f512b0_0;  1 drivers
v0x5c3d06f50c50_0 .net "write", 0 0, v0x5c3d06f51380_0;  1 drivers
E_0x5c3d06f31c50/0 .event negedge, v0x5c3d06f50b90_0;
E_0x5c3d06f31c50/1 .event posedge, v0x5c3d06f50650_0;
E_0x5c3d06f31c50 .event/or E_0x5c3d06f31c50/0, E_0x5c3d06f31c50/1;
    .scope S_0x5c3d06ef1490;
T_0 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c3d06f508e0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x5c3d06f508e0_0;
    %cmpi/s 512, 0, 32;
    %jmp/0xz T_0.1, 5;
    %pushi/vec4 0, 0, 16;
    %ix/getv/s 4, v0x5c3d06f508e0_0;
    %store/vec4a v0x5c3d06f50a10, 4, 0;
    %load/vec4 v0x5c3d06f508e0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5c3d06f508e0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %vpi_call 3 25 "$readmemh", P_0x5c3d06ef16a0, v0x5c3d06f50a10 {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x5c3d06ef1490;
T_1 ;
    %wait E_0x5c3d06f31c50;
    %load/vec4 v0x5c3d06f50b90_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5c3d06f50800_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x5c3d06f50c50_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x5c3d06f50710_0;
    %load/vec4 v0x5c3d06f23290_0;
    %pad/u 11;
    %ix/vec4 3;
    %ix/load 4, 0, 0; Constant delay
    %assign/vec4/a/d v0x5c3d06f50a10, 0, 4;
    %pushi/vec4 0, 0, 16;
    %assign/vec4 v0x5c3d06f50800_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/vec4 v0x5c3d06f50ad0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.4, 8;
    %load/vec4 v0x5c3d06f23290_0;
    %pad/u 11;
    %ix/vec4 4;
    %load/vec4a v0x5c3d06f50a10, 4;
    %assign/vec4 v0x5c3d06f50800_0, 0;
T_1.4 ;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5c3d06f23100;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c3d06f50ed0_0, 0, 1;
    %end;
    .thread T_2;
    .scope S_0x5c3d06f23100;
T_3 ;
    %delay 50000, 0;
    %load/vec4 v0x5c3d06f50ed0_0;
    %inv;
    %store/vec4 v0x5c3d06f50ed0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x5c3d06f23100;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c3d06f512b0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c3d06f51380_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c3d06f51210_0, 0, 1;
    %pushi/vec4 0, 0, 9;
    %store/vec4 v0x5c3d06f50df0_0, 0, 9;
    %pushi/vec4 0, 0, 16;
    %store/vec4 v0x5c3d06f50fa0_0, 0, 16;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c3d06f512b0_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 44 "$display", "\012============================================" {0 0 0};
    %vpi_call 2 45 "$display", "  CONTINUT MEMORIE DUPA INCARCARE program.hex" {0 0 0};
    %vpi_call 2 46 "$display", "============================================\012" {0 0 0};
    %vpi_call 2 48 "$display", "  Addr  |  Hex   |  Decimal  |  Binary" {0 0 0};
    %vpi_call 2 49 "$display", "--------|--------|-----------|------------------" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c3d06f51170_0, 0, 32;
T_4.0 ;
    %load/vec4 v0x5c3d06f51170_0;
    %cmpi/s 32, 0, 32;
    %jmp/0xz T_4.1, 5;
    %vpi_call 2 52 "$display", "  %04h  |  %04h  |  %5d    |  %b", &PV<v0x5c3d06f51170_0, 0, 9>, &A<v0x5c3d06f50a10, v0x5c3d06f51170_0 >, &A<v0x5c3d06f50a10, v0x5c3d06f51170_0 >, &A<v0x5c3d06f50a10, v0x5c3d06f51170_0 > {0 0 0};
    %load/vec4 v0x5c3d06f51170_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5c3d06f51170_0, 0, 32;
    %jmp T_4.0;
T_4.1 ;
    %vpi_call 2 55 "$display", "\012============================================" {0 0 0};
    %vpi_call 2 56 "$display", "  ZONA INSTRUCTIUNI (0x00 - 0x0F)" {0 0 0};
    %vpi_call 2 57 "$display", "============================================\012" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c3d06f51170_0, 0, 32;
T_4.2 ;
    %load/vec4 v0x5c3d06f51170_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_4.3, 5;
    %vpi_call 2 60 "$display", "  Mem[%02d] = 0x%04h", v0x5c3d06f51170_0, &A<v0x5c3d06f50a10, v0x5c3d06f51170_0 > {0 0 0};
    %load/vec4 v0x5c3d06f51170_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5c3d06f51170_0, 0, 32;
    %jmp T_4.2;
T_4.3 ;
    %vpi_call 2 63 "$display", "\012============================================" {0 0 0};
    %vpi_call 2 64 "$display", "  VERIFICARE CITIRE PRIN PORT" {0 0 0};
    %vpi_call 2 65 "$display", "============================================\012" {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5c3d06f51170_0, 0, 32;
T_4.4 ;
    %load/vec4 v0x5c3d06f51170_0;
    %cmpi/s 16, 0, 32;
    %jmp/0xz T_4.5, 5;
    %load/vec4 v0x5c3d06f51170_0;
    %pad/s 9;
    %store/vec4 v0x5c3d06f50df0_0, 0, 9;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5c3d06f51210_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 72 "$display", "  Read Mem[%02d] = 0x%04h (dout)", v0x5c3d06f51170_0, v0x5c3d06f510a0_0 {0 0 0};
    %load/vec4 v0x5c3d06f51170_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5c3d06f51170_0, 0, 32;
    %jmp T_4.4;
T_4.5 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5c3d06f51210_0, 0, 1;
    %vpi_call 2 76 "$display", "\012============================================" {0 0 0};
    %vpi_call 2 77 "$display", "  TEST COMPLET" {0 0 0};
    %vpi_call 2 78 "$display", "============================================\012" {0 0 0};
    %vpi_call 2 80 "$finish" {0 0 0};
    %end;
    .thread T_4;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "memory_load_tb.v";
